    "open-graph-scraper": "^6.9.0",
    "openai": "^5.8.2",
    "pdfjs-dist": "5.3.31",
    "postcss": "8.4.31",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-quilljs": "^2.0.5",
    "react-redux": "^9.2.0",
    "resend": "^4.0.1",
    "sharp": "^0.33.5",
