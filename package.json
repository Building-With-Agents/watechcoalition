{
  "private": true,
  "scripts": {
    "git-info": "node gitInfo.cjs",
    "build": "npm run git-info && cross-env NODE_ENV=production next build",
    "dev": "npm run git-info && cross-env NODE_ENV=development next dev --turbo",
    "dev-no-t": "npm run git-info && cross-env NODE_ENV=development next dev",
    "db:anonymize:dry": "node scripts/anonymize-docker-db.mjs --dry-run",
    "db:anonymize:apply": "cross-env ANONYMIZE_CONFIRM=YES node scripts/anonymize-docker-db.mjs --apply",
    "db:fixtures:export": "node scripts/export-anonymized-fixtures.mjs --outDir prisma/mock-data",
    "db:seed:anonymized": "node prisma/seed-anonymized.mjs --idempotent",
    "prettier": "prettier --write --ignore-unknown .",
    "prettier:check": "prettier --check --ignore-unknown .",
    "start": "cross-env NODE_ENV=production next start",
    "seed": "node -r dotenv/config ./prisma/seed.mjs",
    "lint": "next lint",
    "test": "npm run git-info && vitest",
    "test:ci": "npm run git-info && vitest run --reporter=default --reporter=junit --outputFile=reports/junit.xml",
    "coverage": "vitest run --coverage",
    "db-reset": "prisma migrate reset",
    "db-push": "prisma db push",
    "db-generate": "prisma generate"
  },
  "type": "module",
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
